<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="rectangle" class = "com.sblc.beans.Rectangle" init-method="init" destroy-method="destroy">
		<constructor-arg name="length" value="10"/>
		<property name="width" value="10"/>
	</bean>

	<bean id="circle" class = "com.programaticapproach.beans.Circle" init-method="init" destroy-method="delete">
		<constructor-arg name="radius" value="10"/>
	</bean>
</beans>
